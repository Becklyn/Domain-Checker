{
    "name": "becklyn/domain-checker",
    "type": "cli",
    "description": "Checks domains and their A records.",
    "homepage": "https://github.com/Becklyn/Domain-Checker",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.4",
        "khill/php-duration": "^1.0",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\DomainChecker\\": "src/"
        }
    },
    "bin": [
        "bin/check-domains"
    ]
}
